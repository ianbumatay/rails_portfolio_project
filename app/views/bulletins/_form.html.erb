<%= form_for @bulletin do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :content %>
    <%= f.text_field :content %> 
    <%= f.label :Board %>
    <%= f.collection_select :board_id, Board.all, :id, :title %>
    <%= f.hidden_field :board_id %>
    <%= f.submit %>
     <% end %>