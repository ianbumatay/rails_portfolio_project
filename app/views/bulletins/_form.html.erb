<%= form_for @bulletin do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :content %>
    <%= f.text_field :content %> 
    <%= f.label :Board %>
    <%= f.collection_select :board_id, Board.all, :id, :month %>
    <%= f.hidden_field :board_id %> 
    <%= f.label :rating %> 
    <%= f.number_field :rating, max:5, min:0 %>
    <%= f.submit %>
     <% end %>