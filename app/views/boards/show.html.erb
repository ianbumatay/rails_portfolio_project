
<h2> Bulletins for the month of <%= @board.month %></h2>

<ul>
    <% @board.bulletins.each do |bulletin| %>
      <p><h4> Bulletins: <%= link_to bulletin.month, bulletin_path(bulletin) %></h4> by: <%= bulletin.user.username %></p>
    <% end %>
</ul>

                      

<%= link_to "Create new Board", new_board_path(@board) %> |

<%= link_to "Edit", edit_board_path(@board) %> |

<%= link_to "Back", boards_path %> | <br>


<%= button_to "Delete", board_path(@board), method: :delete %><br> 

